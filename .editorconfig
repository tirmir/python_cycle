#top-most EditorConfig file
root = true

#Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

# Set default charset
#5 space indentation
[*.py]
charset = utf-8
indet_style = space
indent_size = 4

#Tab indentation (no size specified)
[Makefile]
indet_style = tab
